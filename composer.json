{
    "name": "yoast/changelog-generator",
    "license": "GPL-2.0",
    "description": "Creates changelogs based on the passed parameters",
    "type": "library",
    "require": {
        "symfony/console": "^3.3",
        "vlucas/phpdotenv": "^2.4",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/filesystem": "^4.0",
        "symfony/yaml": "^3.4",
        "symfony/process": "^4.0",
        "symfony/stopwatch": "^4.0"
    },
    "autoload": {
        "psr-4": { "Yoast\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Yoast\\Tests\\": "tests/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    }
}
